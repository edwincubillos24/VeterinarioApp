<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.edwinacubillos.veterinarioapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.VaccineActivity.VaccineActivity"
            android:label="@string/title_activity_vaccine"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.owner_detail.OwnerDetailActivity"
            android:label="@string/title_activity_owner_detail"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.pet_detail.PetDetailActivity"
            android:label="@string/title_activity_pet_detail"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.pet_new.PetNewActivity"
            android:label="@string/title_activity_new_pet"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.owner_new.OwnerNewActivity"
            android:label="@string/title_activity_new_owner"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"/>
    </application>
</manifest>